<div class="container">
  <% _.each(artists, function(artist) { %>
  <div class="col-md-4">  
    <a href="/artist/<%= artist.slug %>">
      <% if(artist.images) { %>
      <img src="<%= artist.images.main.url %>" class="img-circle" />
      <% } %>
      <h1><%= artist.name %></h1>
    </a>
  </div>
  <% }); %>
  </div>
</div>
